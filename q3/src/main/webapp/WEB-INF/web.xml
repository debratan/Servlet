<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>sip</servlet-name>
        <servlet-class>servletlogin.sip</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>nextS</servlet-name>
        <servlet-class>servletlogin.nextsip</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>sip</servlet-name>
        <url-pattern>/servletlogin</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>nextS</servlet-name>
        <url-pattern>/nextS</url-pattern>
    </servlet-mapping>
</web-app>
